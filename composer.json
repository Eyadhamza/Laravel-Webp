{
    "name": "pi/laravel-webp",
    "description": "This is my package laravel-webp",
    "keywords": [
        "pi",
        "laravel",
        "laravel-webp"
    ],
    "homepage": "https://github.com/pi/laravel-webp",
    "license": "MIT",
    "authors": [
        {
            "name": "Eyad Hamza",
            "email": "EyadHamza0@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "7.4.*|^8.0",
        "illuminate/contracts": "^8.37",
        "intervention/image": "^2.7",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Pi\\LaravelWebp\\": "src",
            "Pi\\LaravelWebp\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pi\\LaravelWebp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pi\\LaravelWebp\\LaravelWebpServiceProvider"
            ],
            "aliases": {
                "ImageToWebp": "Pi\\LaravelWebp\\ImageToWebp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
